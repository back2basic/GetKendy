use kendynextjs;

db.createUser({
  user: 'ron',
  pwd: 'password',
  roles: [
    {
      role: 'dbOwner',
      db: 'kendynextjs',
    },
  ],
});

use kendynextauth;

db.createUser({
  user: 'ron',
  pwd: 'password',
  roles: [
    {
      role: 'dbOwner',
      db: 'kendynextauth',
    },
  ],
});

